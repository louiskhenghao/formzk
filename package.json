{
  "name": "formzk",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A comprehensive form management library for React, integrating core functionalities with customizable UI components and advanced validation",
  "keywords": [
    "forms",
    "react",
    "form-management",
    "form-builder",
    "input-components",
    "ui-library",
    "react-hook-form",
    "material-ui",
    "validation"
  ],
  "author": "Louis Loo <louiskhenghao@gmail.com> (https://github.com/louiskhenghao)",
  "homepage": "https://github.com/louiskhenghao/formzk#readme",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "dep": "nx dep-graph",
    "format": "prettier --write .",
    "lint": "nx run-many --all --target=lint",
    "example:dev": "yarn start example --port=3333",
    "example:build": "yarn build example",
    "example:run": "yarn start example --prod --port=3333",
    "build:lib:core": "yarn build core",
    "build:lib:mui": "yarn build mui",
    "build:libs": "yarn build:lib:core && yarn build:lib:mui",
    "release": "yarn nx run formzk:version --releaseAs=patch && yarn build:libs",
    "release:minor": "yarn nx run formzk:version --releaseAs=minor && yarn build:libs",
    "release:major": "yarn nx run formzk:version --releaseAs=major && yarn build:libs"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.7.0",
    "@mui/icons-material": "^5.15.21",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@nx/next": "16.6.0",
    "next": "14.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.1",
    "react-is": "18.3.1",
    "styled-components": "5.3.6",
    "tslib": "^2.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@emotion/babel-plugin": "11.11.0",
    "@jscutlery/semver": "^3.1.0",
    "@nx/cypress": "16.6.0",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/js": "16.6.0",
    "@nx/linter": "16.6.0",
    "@nx/react": "16.6.0",
    "@nx/rollup": "16.6.0",
    "@nx/workspace": "16.6.0",
    "@rollup/plugin-url": "^7.0.0",
    "@svgr/rollup": "^8.0.1",
    "@swc/plugin-emotion": "^2.5.67",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.196",
    "@types/node": "18.14.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-jest": "^29.4.1",
    "core-js": "^3.32.0",
    "cypress": "^12.16.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "13.4.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "16.6.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
}
